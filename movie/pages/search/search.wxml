<!--pages/search/search.wxml-->
<view class="search">
    <icon type="search" size="20" color="#405f80"></icon>
    <input type="text" confirm-type="search" bindconfirm="onConfirm"/>
</view>

<view class="container">
    <block wx:for="{{movies}}" wx:key="index">
       <v-item data="{{item}}"></v-item>
    </block>
</view>
