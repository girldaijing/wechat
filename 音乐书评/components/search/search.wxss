/* components/search/search.wxss */
.header{
    width: 100%;
    height: 40px;
    display: flex;
    align-items: center;
    background: #fafafa;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 200;
}
button{
    width: 66px;
    height: 25px;
    line-height: 25px;
    border-radius: 50px;
    background: #fff;
    border: 1px solid #999;
    position: absolute;
    right: 4px;
}
.clear{
    position: absolute;
    right: 73px;
}
.container{
    margin-left: 10px;
    margin-top: 60px;
}
.history,.hots{
    margin-top: 30px;
    width: 85px;
    height: 21px;
    line-height: 21px;
    text-align: center;
    border-left: 5px solid black;
    font-size: 18px;
}
.list{
    margin-top: 150rpx;
    padding-left:90rpx;
    padding-right:90rpx;
    display: flex !important;
    flex-wrap: wrap;
    justify-content: space-between !important;
}
v-book{
    margin-bottom: 60rpx;
}
.space{
    display: block;
    position: fixed;
    top:50%;
    width: 100%;
    z-index: 300;
    text-align: center;
}
.scroll-view{
    height:500px;
}